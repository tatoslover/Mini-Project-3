{
  "name": "barkend",
  "version": "1.0.0",
  "description": "Barkend - Your Ultimate Dog Paradise. Discover, explore, and manage dog breeds with the Dog API",
  "main": "barkend-server.js",
  "scripts": {
    "start": "node barkend-server.js",
    "dev": "nodemon barkend-server.js",
    "barkend": "node start-barkend.js",
    "frontend": "cd frontend && python -m http.server 8080",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix",
    "netlify:dev": "netlify dev",
    "netlify:build": "npm run build",
    "build": "echo 'Build complete'"
  },
  "keywords": [
    "dogs",
    "breeds",
    "api",
    "frontend",
    "express",
    "barkend",
    "dog-api",
    "animals",
    "pets"
  ],
  "author": "Barkend Team",
  "license": "MIT",
  "dependencies": {
    "@netlify/functions": "^4.1.8",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.1",
    "netlify-cli": "^22.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/barkend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/barkend/issues"
  },
  "homepage": "https://github.com/yourusername/barkend#readme"
}
