        ],
        "responses": {
          "200": {
            "description": "Dog details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DogResponse"
                }
              }
            }
          },
          "404": {
            "description": "Dog not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Update Dog",
        "description": "Update a specific dog's information",
        "tags": ["CRUD Operations"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Dog ID",
            "required": true,
            "schema": {
              "type": "string"
            }
